{
  "project": {
    "name": "circuit-breaker-test",
    "description": "Test configuration for circuit breaker functionality",
    "version": "1.0.0"
  },
  "repositories": [
    {
      "name": "dotnet-docs",
      "url": "https://github.com/dotnet/docs.git",
      "branch": "main",
      "authType": "none",
      "paths": ["/docs/core"],
      "exclude": ["*/samples/*", "*/media/*"],
      "syncInterval": 600,
      "priority": "high",
      "category": "dotnet"
    }
  ],
  "contextGeneration": {
    "strategies": ["hybrid"],
    "maxChunks": 10,
    "priorityWeighting": {
      "high": 2.0,
      "medium": 1.0,
      "low": 0.5
    }
  },
  "vectorStore": {
    "type": "qdrant",
    "qdrant": {
      "url": "http://localhost:6333",
      "collectionName": "circuit_breaker_test",
      "vectorSize": 384,
      "distance": "Cosine"
    }
  },
  "embedding": {
    "provider": "local",
    "model": "Xenova/all-MiniLM-L6-v2",
    "chunkSize": 1000,
    "chunkOverlap": 200
  },
  "security": {
    "enableInputSanitization": true,
    "enableRateLimiting": true,
    "rateLimitRequests": 50,
    "rateLimitWindow": 60000
  },
  "monitoring": {
    "enableMetrics": true,
    "enablePerformanceTracking": true,
    "logLevel": "info"
  }
}