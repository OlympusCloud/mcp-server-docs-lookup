{
  "project": {
    "name": "test-azure-docs",
    "description": "Test Azure docs sync",
    "version": "1.0.0"
  },
  "repositories": [
    {
      "name": "azure-docs-test",
      "url": "https://github.com/MicrosoftDocs/azure-docs.git",
      "branch": "main",
      "authType": "none",
      "paths": ["/articles/app-service"],
      "exclude": [
        "*/media/*",
        "*/includes/*", 
        "*.png",
        "*.jpg",
        "*.gif",
        "*/sample-code/*",
        "*/samples/*"
      ],
      "recursive": true,
      "filePattern": "**/*.{md,markdown}",
      "syncInterval": 0,
      "priority": "medium",
      "category": "azure",
      "maxDepth": 2
    }
  ],
  "contextGeneration": {
    "strategies": ["semantic"],
    "maxChunks": 20
  },
  "vectorStore": {
    "type": "qdrant",
    "qdrant": {
      "url": "http://localhost:6333",
      "collectionName": "test_docs"
    }
  }
}