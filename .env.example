# Environment configuration for MCP Docs Server
# Copy this file to .env to customize your deployment

# Server Configuration
NODE_ENV=production
LOG_LEVEL=info
SERVER_PORT=3000
SERVER_MODE=api

# Server Features
ENABLE_WEBSOCKET=false
ENABLE_METRICS=true
MAX_MEMORY=2048

# Vector Database
QDRANT_URL=http://qdrant:6333

# For development, you can override these:
# NODE_ENV=development
# LOG_LEVEL=debug
# SERVER_MODE=enhanced
# ENABLE_WEBSOCKET=true
